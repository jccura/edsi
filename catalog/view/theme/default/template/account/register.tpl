function register() {	var valid = 1;	var msg = "";	var firstname = $('#firstname').val();	var middlename = $('#firstname').val();	var lastname = $('#firstname').val();	var mobile = $('#mobile').val();	var email = $('#email').val();	var username = $('#username').val();	var password = $('#password').val();	var cpassword = $('#cpassword').val();	var email = $('#email').val();		if(firstname == "") {		valid = 0;		msg += "Firstname is mandatory.<br>";	}		if(middlename == "") {		valid = 0;		msg += "Middlename is mandatory.<br>";	}		if(lastname == "") {		valid = 0;		msg += "Lastname is mandatory.<br>";	}	if(email == "") {		valid = 0;		msg += "Email is mandatory.<br>";	}		if(mobile == "") {		valid = 0;		msg += "Phone Number is mandatory.<br>";	}		if(username == "") {		valid = 0;		msg += "Username is mandatory.<br>";	}		if(password == "") {		valid = 0;		msg += "Password is mandatory.<br>";	}		if(password != cpassword) {		valid = 0;		msg += "Passwords does not match.<br>";	}		if($('#sponsor_id').val() == 0) {		valid = 0;		msg += "Sponsor is not valid.<br>";	}		if (!($('#accept').is(":checked"))){		valid = 0;		msg += "You have to accept the terms and conditions.<br>";	}		if(valid == 1) {		$("#task").val("add");		$('#form').attr('action', 'register');		$('#form').submit();	} else {		$('#msg').html(msg);			      		$(function() {			$("#dialog-message").dialog({				modal: true,				width: 300,				buttons: {					Ok: function() {						$(this).dialog("close");					}				}			});		});	}}
